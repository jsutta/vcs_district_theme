{#
/**
 * @file
 * Students Page
 *
 * The doctype, html, head and body tags are not in this template. Instead
 * they can be found in the html.html.twig template in this directory.
 *
 * See node.html.twig for available variables
#}

{% set container = theme.settings.fluid_container ? 'container-fluid' : 'container' %}

{# Announcements #}
{% include district_theme_directory ~ '/templates/regions/announcements.html.twig' %}
{# Main header #}
{% include district_theme_directory ~ '/templates/regions/header.html.twig' %}
{# Page header #}
{% include 'page-header.html.twig' %}

{# Main Content #}
<div role="main" class="content-wrapper--audience-page main-container js-quickedit-main-content">
  <a id="main-content"></a>
  <div class="{{ container }}">
    <section class="row content-section clearfix">
      {# Featured #}
      {% include district_theme_directory ~ '/templates/regions/featured-pages.html.twig' %}
      {# Ad Space #}
      {% include district_theme_directory ~ '/templates/regions/adspace.html.twig' %}
    </section>
  </div>
  <div class="wrapper--highlights">
    {# Highlights #}
    {% include district_theme_directory ~ '/templates/regions/highlights.html.twig' %}
  </div>
  <div class="{{ container }}">
    {# Resources #}
    <section class="row content-section clearfix">
      {% include district_theme_directory ~ '/templates/regions/resources.html.twig' %}
    </section>
  </div>
</div>

{# District Programs #}
{% include district_theme_directory ~ '/templates/regions/district-programs.html.twig' %}

{# Footer #}
{% include district_theme_directory ~ '/templates/regions/footer.html.twig' %}
