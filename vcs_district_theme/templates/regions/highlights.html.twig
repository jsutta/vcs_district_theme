{# Highlights #}
{% if is_front %}
  <div class="bg-overlay"></div>
  <div class="container">
    {% include district_theme_directory ~ '/templates/regions/annual-theme.html.twig' %}
    <div class="vision-statement">
      {{ site_slogan|t }}
    </div>
  </div>
  <section class="district-highlights">
    <div class="container">
      <div class="row">
        <div class="col-xs-12">
          {{ page.highlights }}
        </div>
      </div>
    </div>
  </section>
{% else %}
  {% if page.highlights %}
    {% block highlights %}
      {{ page.highlights }}
    {% endblock %}
  {% endif %}
{% endif %}
