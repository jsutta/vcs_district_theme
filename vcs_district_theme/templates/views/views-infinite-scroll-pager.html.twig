{#
/**
 * @file
 * The views infinite scroll pager template.
 */
#}

{% if items.next %}
  <ul{{ attributes }}>
    <li class="pager__item">
      <span>
        <a class="btn btn-primary" href="{{ items.next.href }}" title="{{ 'Go to next page'|t }}" rel="next">{{ options.button_text }}</a>
      </span>
    </li>
  </ul>
{% endif %}
